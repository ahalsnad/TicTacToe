<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<BorderPane prefHeight="200.0" prefWidth="200.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
    <center>
        <AnchorPane>
            <children>
                <GridPane fx:id="grid" alignment="CENTER" gridLinesVisible="true" hgap="1.0" layoutX="1.0"
                          layoutY="92.0" prefHeight="311.0" prefWidth="301.0" vgap="1.0">
                    <rowConstraints>
                        <RowConstraints maxHeight="104.0" minHeight="97.0" prefHeight="103.0"/>
                        <RowConstraints maxHeight="122.0" minHeight="51.0" prefHeight="105.0"/>
                        <RowConstraints maxHeight="100.0" minHeight="10.0" prefHeight="100.0"/>
                    </rowConstraints>
                    <columnConstraints>
                        <ColumnConstraints minWidth="10.0" prefWidth="100.0"/>
                        <ColumnConstraints minWidth="10.0" prefWidth="100.0"/>
                        <ColumnConstraints minWidth="10.0" prefWidth="100.0"/>
                    </columnConstraints>
                    <children>
                        <TextField fx:id="id00" alignment="CENTER" editable="false" onMouseClicked="#printTurn00"
                                   prefHeight="108.0" prefWidth="99.0">
                            <font>
                                <Font name="Times New Roman Bold" size="24.0"/>
                            </font>
                        </TextField>
                        <TextField fx:id="id02" alignment="CENTER" editable="false" onMouseClicked="#printTurn02"
                                   prefHeight="103.0" prefWidth="99.0" GridPane.rowIndex="2">
                            <font>
                                <Font name="Times New Roman Bold" size="24.0"/>
                            </font>
                        </TextField>
                        <TextField fx:id="id11" alignment="CENTER" editable="false" onMouseClicked="#printTurn11"
                                   prefHeight="104.0" prefWidth="99.0" GridPane.columnIndex="1" GridPane.rowIndex="1">
                            <font>
                                <Font name="Times New Roman Bold" size="24.0"/>
                            </font>
                        </TextField>
                        <TextField fx:id="id10" alignment="CENTER" editable="false" onMouseClicked="#printTurn10"
                                   prefHeight="105.0" prefWidth="100.0" GridPane.columnIndex="1">
                            <font>
                                <Font name="Times New Roman Bold" size="24.0"/>
                            </font>
                        </TextField>
                        <TextField fx:id="id12" alignment="CENTER" editable="false" onMouseClicked="#printTurn12"
                                   prefHeight="101.0" prefWidth="99.0" GridPane.columnIndex="1" GridPane.rowIndex="2">
                            <font>
                                <Font name="Times New Roman Bold" size="24.0"/>
                            </font>
                        </TextField>
                        <TextField fx:id="id21" alignment="CENTER" editable="false" onMouseClicked="#printTurn21"
                                   prefHeight="108.0" prefWidth="100.0" GridPane.columnIndex="2" GridPane.rowIndex="1">
                            <font>
                                <Font name="Times New Roman Bold" size="24.0"/>
                            </font>
                        </TextField>
                        <TextField fx:id="id20" alignment="CENTER" editable="false" onMouseClicked="#printTurn20"
                                   prefHeight="108.0" prefWidth="100.0" GridPane.columnIndex="2">
                            <font>
                                <Font name="Times New Roman Bold" size="24.0"/>
                            </font>
                        </TextField>
                        <TextField fx:id="id22" alignment="CENTER" editable="false" onMouseClicked="#printTurn22"
                                   prefHeight="106.0" prefWidth="100.0" GridPane.columnIndex="2" GridPane.rowIndex="2">
                            <font>
                                <Font name="Times New Roman Bold" size="24.0"/>
                            </font>
                        </TextField>
                        <TextField fx:id="id01" alignment="CENTER" editable="false" onMouseClicked="#printTurn01"
                                   prefHeight="109.0" prefWidth="99.0" GridPane.rowIndex="1">
                            <font>
                                <Font name="Times New Roman Bold" size="24.0"/>
                            </font>
                        </TextField>
                    </children>
                </GridPane>
                <Label fx:id="label" alignment="CENTER" contentDisplay="CENTER" layoutX="24.0" layoutY="36.0"
                       prefHeight="20.0" prefWidth="254.0" textAlignment="CENTER">
                    <font>
                        <Font name="Times New Roman" size="18.0"/>
                    </font>
                </Label>

            </children>
        </AnchorPane>
    </center>
    <top>
        <MenuBar BorderPane.alignment="CENTER">
            <menus>
                <Menu mnemonicParsing="false" text="Game">
                    <items>
                        <MenuItem fx:id="newGame" mnemonicParsing="false" onAction="#newGame" text="New Game"/>
                        <MenuItem mnemonicParsing="false" onAction="#replay" text="Restart"/>
                        <MenuItem mnemonicParsing="false" onAction="#close" text="Exit"/>
                    </items>
                </Menu>
            </menus>
        </MenuBar>
    </top>
</BorderPane>
